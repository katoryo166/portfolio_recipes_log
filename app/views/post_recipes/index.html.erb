<div class="row">
  <div class="col-md-10 col-xs-4 offset-md-1">
    <h2>投稿一覧</h2>
  </div>
</div>
<div class="search_form">
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :keyword, value: @keyword %>
    <%= f.submit "検索" %>
  <% end %>
</div>
<div class="post-list">

</div>
<div class="row">
  <%= render 'post_recipes/post_recipes', post_recipes: @post_recipes %>
</div>

